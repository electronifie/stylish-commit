{
  "name": "stylish-commit",
  "version": "0.0.1",
  "description": "Check staged changes with style scripts before commiting with git.",
  "repository": "https://github.com/electronifie/stylish-commit",
  "license": "ISC",
  "main": "index.js",
  "bin": "bin/stylish-commit",
  "scripts": {
    "test": "mocha test",
    "postinstall": "./bin/stylish-commit --install-hook",
    "uninstall": "./bin/stylish-commit --uninstall-hook"
  },
  "keywords": [
    "git",
    "pre-commit",
    "styleguide",
    "stylecheck",
    "lint",
    "hint",
    "jslint",
    "jshint"
  ],
  "devDependencies": {
    "chai": "^3.0.0",
    "fs-extra": "^0.20.1",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "argparse": "^1.0.2",
    "colors": "^1.1.2",
    "colors.js": "^1.2.4",
    "debug": "^2.2.0",
    "diff": "^1.4.0",
    "handlebars": "^3.0.3",
    "inquirer": "^0.8.5",
    "lodash": "^3.9.3",
    "minimatch": "^2.0.8",
    "parse-diff": "0.0.15",
    "require-all": "^1.1.0",
    "sync-exec": "^0.5.0"
  }
}
